    },
    {
      "path": "dist/tyrann-io.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@size-limit/preset-small-lib": "^4.10.2",
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "morgan": "^1.10.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/string-format": "^2.0.0",
    "axios": "^0.21.1",
    "fp-ts": "^2.10.5",
    "io-ts": "^2.2.16",
    "query-string": "^7.0.0",
    "string-format": "^2.0.0"
  },
  "resolutions": {
    "**/typescript": "^4.2.4"
  }
